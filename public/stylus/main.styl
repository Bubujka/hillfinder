html 
  height 100% 
body  
  height 100%
  margin 0
  padding 0
#map_canvas 
  height 100%

.finder-pane
  z-index 3
  position absolute
  right 100px
  top 40px

  &__last5
    padding 15px 0px
    width: 350px
    background white
    margin-top 10px
    border-radius 3px
    table
      font-size: 12px
      width 300px
      position: relative
      left 20px
    td
      padding-left 20px
      padding-top 1px
      padding-bottom 3px
    th
      padding-bottom 8px
      text-align center
    &__nothing
      text-align center
    &__data
      text-align left
    &__hr
      border-top 1px dotted black
      padding 0px
    &__point-vals
      text-align center
    &__title
      font-family georgia
      font-style italic
      font-size 23px
      padding 10px 0px
      text-align center
    &__time
      display: inline-block
      color: #666
    &__ip
      padding-left 20px
      display: inline-block
      color: #666
  &__inputs
    span
      width 120px
      font-size: 18px
      font-family arial
      display inline-block
      text-shadow 1px 1px 2px rgba(250,250,250,1)
    input
      padding: 7px 10px
      font-size 14px
  &__height
    font-size 13px
    padding 30px 0px
    display none
    &__big
      font-family georgia
      font-style italic
      font-weight normal
      font-size 46px
      text-align center
      padding 10px 0px
    
    width 300px
  &__button
    padding 7px 10px
  
